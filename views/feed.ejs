            <% posts.forEach(post => { %>
              <div class="gallery-item">
                <figure>
                  <img src="/images/uploads/<%= post.image %>" alt="img" />
                </figure>
                <p><%= post.imgText %></p>
              </div>
            <% }); %>
          <% } %>  
    
